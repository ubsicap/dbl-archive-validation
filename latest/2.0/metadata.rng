          <ref name="translationManifestResource"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="nonTranslationManifestContainer">
    <element name="container">
      <attribute name="uri">
        <data type="anyURI"/>
      </attribute>
      <oneOrMore>
        <choice>
          <ref name="nonTranslationManifestContainer"/>
          <ref name="nonTranslationManifestResource"/>
        </choice>
      </oneOrMore>
    </element>
