      </optional>
      <oneOrMore>
        <choice>
          <ref name="sourceStructureDivision"/>
          <ref name="sourceStructureContent"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="sourceStructureContent">
