<?xml version="1.0" encoding="UTF-8"?>
<!--
  metadata.rnc
  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
  A compact syntax Relax NG Schema for the DBL @type="audio" metadata file.
  DBL audio bundle version = 1.2
-->
<!--
  - Suggested Dublin Core Metadata (DCMI) element/refinement relationships are 
    added as documentation strings "## DCMI: "
  - A syntax encoding scheme may be mentioned after the DCMI documentation in 
    parentheses; e.g. language/iso (ISO639-3)
  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
-->
<grammar ns="" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <ref name="m.DBLMetadata"/>
  </start>
  <div>
    <define name="m._.contentType.enum">
      <value>xhtml</value>
    </define>
    <define name="m._.contentType">
      <attribute name="contentType">
        <ref name="m._.contentType.enum"/>
      </attribute>
    </define>
    <define name="m._.code">
      <attribute name="code">
        <ref name="bookCode.enum"/>
      </attribute>
    </define>
    <define name="m._datatype.string2">
      <data type="string">
        <param name="minLength">2</param>
      </data>
    </define>
  </div>
  <div>
    <define name="m.DBLMetadata.type">
      <attribute name="type">
        <value>audio</value>
      </attribute>
    </define>
    <define name="m.DBLMetadata.typeVersion">
      <attribute name="typeVersion">
        <value>1.3</value>
      </attribute>
    </define>
    <define name="m.DBLMetadata.id">
      <attribute name="id">
        <a:documentation>id is overwritten at submission</a:documentation>
        <data type="string">
          <param name="pattern">[a-f0-9]{16}</param>
        </data>
      </attribute>
    </define>
    <define name="m.DBLMetadata.revision">
      <attribute name="revision">
        <data type="integer"/>
      </attribute>
    </define>
    <define name="m.DBLMetadata.attlist">
      <interleave>
        <ref name="m.DBLMetadata.type"/>
        <ref name="m.DBLMetadata.typeVersion"/>
        <ref name="m.DBLMetadata.id"/>
        <ref name="m.DBLMetadata.revision"/>
      </interleave>
    </define>
    <define name="m.DBLMetadata">
      <element name="DBLMetadata">
        <interleave>
          <ref name="m.DBLMetadata.attlist"/>
          <ref name="m.identification"/>
          <ref name="m.confidential"/>
          <optional>
            <ref name="m.relationships"/>
          </optional>
          <ref name="m.agencies"/>
          <ref name="m.language"/>
          <ref name="m.country"/>
          <ref name="m.type"/>
          <ref name="m.format"/>
          <ref name="m.books"/>
          <ref name="m.copyright"/>
          <optional>
            <ref name="m.promotion"/>
          </optional>
          <ref name="m.archiveStatus"/>
        </interleave>
      </element>
    </define>
  </div>
  <div>
    <define name="m.identification">
      <element name="identification">
        <interleave>
          <ref name="m.name"/>
          <ref name="m.nameLocal"/>
          <ref name="m.scope"/>
          <ref name="m.description"/>
          <ref name="m.dateCompleted"/>
          <zeroOrMore>
            <ref name="m.systemId"/>
          </zeroOrMore>
          <ref name="m.bundleProducer"/>
        </interleave>
      </element>
    </define>
  </div>
  <div>
    <define name="m.name">
      <element name="name">
        <a:documentation>DCMI: title</a:documentation>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.nameLocal">
      <element name="nameLocal">
        <a:documentation>nameLocal should default to name</a:documentation>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.scope">
      <element name="scope">
        <a:documentation>DCMI: title/scriptureScope</a:documentation>
        <text/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.description">
      <element name="description">
        <a:documentation>DCMI: description</a:documentation>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.dateCompleted">
      <element name="dateCompleted">
        <a:documentation>DCMI: date</a:documentation>
        <optional>
          <choice>
            <data type="date"/>
            <data type="gYear"/>
            <data type="gYearMonth"/>
            <data type="dateTime"/>
          </choice>
        </optional>
      </element>
    </define>
  </div>
  <div>
    <define name="m.systemId.type.enum">
      <choice>
        <value>gbc</value>
        <a:documentation>Global Bible Catalogue entry ID</a:documentation>
        <value>tms</value>
        <a:documentation>UBS Translation Management System database ID</a:documentation>
        <value>reap</value>
        <a:documentation>SIL REAP database ID</a:documentation>
        <value>dbp</value>
        <a:documentation>FCBH Digital Bible Platform DAM ID</a:documentation>
      </choice>
    </define>
    <define name="m.systemId.type">
      <attribute name="type">
        <ref name="m.systemId.type.enum"/>
      </attribute>
    </define>
    <define name="m.systemId.attlist">
      <ref name="m.systemId.type"/>
    </define>
    <define name="m.systemId">
      <element name="systemId">
        <ref name="m.systemId.attlist"/>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.bundleProducer">
      <element name="bundleProducer">
        <a:documentation>bundleProducer is overwritten by Paratext</a:documentation>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.confidential">
      <element name="confidential">
        <a:documentation>DCMI: accessRights/confidential</a:documentation>
        <data type="boolean"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.relationships">
      <element name="relationships">
        <oneOrMore>
          <ref name="m.relation"/>
        </oneOrMore>
      </element>
    </define>
  </div>
  <div>
    <define name="m.relation.type.enum">
      <choice>
        <value>text</value>
        <value>print</value>
        <value>audio</value>
        <value>video</value>
        <value>ebook</value>
      </choice>
    </define>
    <define name="m.relation.type">
      <attribute name="type">
        <ref name="m.relation.type.enum"/>
      </attribute>
    </define>
    <define name="m.relation.id">
      <attribute name="id">
        <data type="string">
          <param name="pattern">[a-f0-9]{16}</param>
        </data>
      </attribute>
    </define>
    <define name="m.relation.revision">
      <attribute name="revision">
        <data type="integer"/>
      </attribute>
    </define>
    <define name="m.relation.attlist">
      <interleave>
        <ref name="m.relation.type"/>
        <ref name="m.relation.id"/>
        <ref name="m.relation.revision"/>
      </interleave>
    </define>
    <define name="m.relation">
      <element name="relation">
        <a:documentation>DCMI: relation</a:documentation>
        <ref name="m.relation.attlist"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.agencies">
      <element name="agencies">
        <interleave>
          <oneOrMore>
            <ref name="m.rightsHolder"/>
          </oneOrMore>
          <optional>
            <ref name="m.rightsAdmin"/>
          </optional>
          <oneOrMore>
            <ref name="m.contributor"/>
          </oneOrMore>
        </interleave>
      </element>
    </define>
  </div>
  <div>
    <define name="m.rightsHolder.abbr">
      <attribute name="abbr">
        <ref name="m._datatype.string2"/>
      </attribute>
    </define>
    <define name="m.rightsHolder.url">
      <attribute name="url">
        <data type="anyURI"/>
      </attribute>
    </define>
    <define name="m.rightsHolder.local">
      <attribute name="local">
        <ref name="m._datatype.string2"/>
      </attribute>
    </define>
    <define name="m.rightsHolder.attlist">
      <interleave>
        <optional>
          <ref name="m.rightsHolder.abbr"/>
        </optional>
        <optional>
          <ref name="m.rightsHolder.url"/>
        </optional>
        <optional>
          <ref name="m.rightsHolder.local"/>
        </optional>
      </interleave>
    </define>
    <define name="m.rightsHolder">
      <element name="rightsHolder">
        <a:documentation>DCMI: rightsHolder</a:documentation>
        <ref name="m.rightsHolder.attlist"/>
        <attribute name="uid">
          <ref name="m._datatype.string2"/>
        </attribute>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.rightsAdmin.url">
      <attribute name="url">
        <data type="anyURI"/>
      </attribute>
    </define>
    <define name="m.rightsAdmin.attlist">
      <optional>
        <ref name="m.rightsAdmin.url"/>
      </optional>
    </define>
    <define name="m.rightsAdmin">
      <element name="rightsAdmin">
        <a:documentation>DCMI: rightsHolder</a:documentation>
        <ref name="m.rightsAdmin.attlist"/>
        <attribute name="uid">
          <ref name="m._datatype.string2"/>
        </attribute>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.contributor.content">
      <attribute name="content">
        <data type="boolean"/>
      </attribute>
    </define>
    <define name="m.contributor.publication">
      <attribute name="publication">
        <data type="boolean"/>
      </attribute>
    </define>
    <define name="m.contributor.management">
      <attribute name="management">
        <data type="boolean"/>
      </attribute>
    </define>
    <define name="m.contributor.finance">
      <attribute name="finance">
        <data type="boolean"/>
      </attribute>
    </define>
    <define name="m.contributor.qa">
      <attribute name="qa">
        <data type="boolean"/>
      </attribute>
    </define>
    <define name="m.contributor.attlist">
      <oneOrMore>
        <choice>
          <ref name="m.contributor.content"/>
          <ref name="m.contributor.publication"/>
          <ref name="m.contributor.management"/>
          <ref name="m.contributor.finance"/>
          <ref name="m.contributor.qa"/>
        </choice>
      </oneOrMore>
    </define>
    <define name="m.contributor">
      <element name="contributor">
        <a:documentation>DCMI: contributor</a:documentation>
        <optional>
          <ref name="m.contributor.attlist"/>
        </optional>
        <attribute name="uid">
          <ref name="m._datatype.string2"/>
        </attribute>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.language">
      <element name="language">
        <interleave>
          <ref name="m.language.iso"/>
          <ref name="m.language.name"/>
          <ref name="m.language.nameLocal"/>
          <ref name="m.ldml"/>
          <ref name="m.rod"/>
        </interleave>
      </element>
    </define>
  </div>
  <div>
    <define name="m.language.iso">
      <element name="iso">
        <a:documentation>DCMI: language/iso (ISO639-3)</a:documentation>
        <data type="string">
          <param name="pattern">[a-z][a-z][a-z]</param>
        </data>
      </element>
    </define>
  </div>
  <div>
    <define name="m.language.name">
      <element name="name">
        <a:documentation>DCMI: subject/language (ISO639-3)</a:documentation>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.language.nameLocal">
      <element name="nameLocal">
        <a:documentation>DCMI: subject/language (ISO639-3)</a:documentation>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.ldml">
      <element name="ldml">
        <a:documentation>DCMI: language/ldml</a:documentation>
        <optional>
          <data type="string">
            <param name="pattern">[A-Za-z]{2,3}([\-_][A-Za-z0-9]+){0,3}</param>
          </data>
        </optional>
      </element>
    </define>
  </div>
  <div>
    <define name="m.rod">
      <element name="rod">
        <a:documentation>DCMI: language/rod</a:documentation>
        <optional>
          <data type="string">
            <param name="pattern">[0-9]{5}</param>
          </data>
        </optional>
      </element>
    </define>
  </div>
  <div>
    <define name="m.country">
      <element name="country">
        <interleave>
          <ref name="m.country.iso"/>
          <ref name="m.country.name"/>
        </interleave>
      </element>
    </define>
  </div>
  <div>
    <define name="m.country.iso">
      <element name="iso">
        <a:documentation>DCMI: coverage/spatial (ISO3166)</a:documentation>
        <data type="string">
          <param name="pattern">[A-Z][A-Z][A-Z]?</param>
        </data>
      </element>
    </define>
  </div>
  <div>
    <define name="m.country.name">
      <element name="name">
        <a:documentation>DCMI: subject/subjectCountry</a:documentation>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.type">
      <element name="type">
        <ref name="m.audioType"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.audioType.enum">
      <choice>
        <value>Dramatized</value>
        <value>Non-Dramatized</value>
        <value>Single-Voice</value>
      </choice>
    </define>
    <define name="m.audioType">
      <element name="audioType">
        <a:documentation>DCMI: type/audioType</a:documentation>
        <optional>
          <ref name="m.audioType.enum"/>
        </optional>
      </element>
    </define>
  </div>
  <div>
    <define name="m.format">
      <element name="format">
        <a:documentation>DCMI: format</a:documentation>
        <interleave>
          <ref name="m.mimeType"/>
          <ref name="m.compression"/>
          <ref name="m.trackConfiguration"/>
          <ref name="m.bitRate"/>
          <ref name="m.bitDepth"/>
          <ref name="m.samplingRate"/>
        </interleave>
      </element>
    </define>
  </div>
  <div>
    <define name="m.mimeType.enum">
      <choice>
        <value>audio/mpeg</value>
        <value>audio/wav</value>
      </choice>
    </define>
    <define name="m.mimeType">
      <element name="mimeType">
        <ref name="m.mimeType.enum"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.compression.enum">
      <choice>
        <value>MP3</value>
        <value>FLAC</value>
        <value>WAV</value>
        <empty/>
      </choice>
    </define>
    <define name="m.compression">
      <a:documentation>http://www.ebu.ch/metadata/cs/web/ebu_AudioCompressionCodeCS_p.xml.htm</a:documentation>
      <element name="compression">
        <ref name="m.compression.enum"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.trackConfiguration.enum">
      <choice>
        <value>1/0 (Mono)</value>
        <value>Dual mono</value>
        <value>2/0 (Stereo)</value>
        <value>5.1 Surround</value>
        <empty/>
      </choice>
    </define>
    <define name="m.trackConfiguration">
      <a:documentation>http://www.ebu.ch/metadata/cs/web/ebu_AudioFormatCodeCS_p.xml.htm</a:documentation>
      <element name="trackConfiguration">
        <ref name="m.trackConfiguration.enum"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.bitRate">
      <element name="bitRate">
        <choice>
          <data type="integer"/>
          <empty/>
        </choice>
      </element>
    </define>
  </div>
  <div>
    <define name="m.bitDepth">
      <element name="bitDepth">
        <choice>
          <data type="integer"/>
          <empty/>
        </choice>
      </element>
    </define>
  </div>
  <div>
    <define name="m.samplingRate">
      <element name="samplingRate">
        <choice>
          <data type="integer"/>
          <empty/>
        </choice>
      </element>
    </define>
  </div>
  <div>
    <define name="m.books">
      <element name="books">
        <oneOrMore>
          <ref name="m.book"/>
        </oneOrMore>
      </element>
    </define>
  </div>
  <div>
    <define name="m.book.attlist">
      <ref name="m._.code"/>
    </define>
    <define name="m.book">
      <element name="book">
        <ref name="m.book.attlist"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.copyright">
      <element name="copyright">
        <ref name="m.statement"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.statement.attlist">
      <ref name="m._.contentType"/>
    </define>
    <define name="m.statement">
      <element name="statement">
        <a:documentation>For example: (C)2002, 2012 United Bible Societies
DCMI: rights</a:documentation>
        <ref name="m.statement.attlist"/>
        <oneOrMore>
          <ref name="m.htmlMarkup"/>
        </oneOrMore>
      </element>
    </define>
  </div>
  <div>
    <define name="m.promotion">
      <element name="promotion">
        <ref name="m.promoVersionInfo"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.promoVersionInfo.attlist">
      <ref name="m._.contentType"/>
    </define>
    <define name="m.promoVersionInfo">
      <element name="promoVersionInfo">
        <a:documentation>DCMI: description/pubPromoVersionInfo</a:documentation>
        <ref name="m.promoVersionInfo.attlist"/>
        <oneOrMore>
          <ref name="m.htmlMarkup"/>
        </oneOrMore>
      </element>
    </define>
  </div>
  <div>
    <define name="m.archiveStatus">
      <element name="archiveStatus">
        <interleave>
          <ref name="m.archivistName"/>
          <ref name="m.dateArchived"/>
          <ref name="m.dateUpdated"/>
          <ref name="m.comments"/>
        </interleave>
      </element>
    </define>
  </div>
  <div>
    <define name="m.archivistName">
      <element name="archivistName">
        <a:documentation>archivistName is overwritten by Paratext
DCMI: contributor/archivist</a:documentation>
        <ref name="m._datatype.string2"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.dateArchived">
      <element name="dateArchived">
        <a:documentation>dateArchived is overwritten by Paratext
DCMI: dateSubmitted (xsd:dateTime)</a:documentation>
        <data type="dateTime"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.dateUpdated">
      <element name="dateUpdated">
        <a:documentation>dateUpdated is overwritten by Paratext
DCMI: modified (xsd:dateTime)</a:documentation>
        <data type="dateTime"/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.comments">
      <element name="comments">
        <a:documentation>DCMI: abstract</a:documentation>
        <data type="string">
          <param name="minLength">2</param>
          <param name="maxLength">255</param>
        </data>
      </element>
    </define>
  </div>
  <div>
    <define name="m.htmlMarkup">
      <choice>
        <ref name="m.p"/>
        <ref name="m.h1"/>
        <ref name="m.h2"/>
        <ref name="m.h3"/>
        <ref name="m.ul"/>
        <ref name="m.ol"/>
        <ref name="m.blockquote"/>
        <ref name="m.htmlCharMarkup"/>
      </choice>
    </define>
    <define name="m.p">
      <element name="p">
        <oneOrMore>
          <choice>
            <text/>
            <ref name="m.htmlCharMarkup"/>
          </choice>
        </oneOrMore>
      </element>
    </define>
    <define name="m.h1">
      <element name="h1">
        <text/>
      </element>
    </define>
    <define name="m.h2">
      <element name="h2">
        <text/>
      </element>
    </define>
    <define name="m.h3">
      <element name="h3">
        <text/>
      </element>
    </define>
    <define name="m.ol">
      <element name="ol">
        <oneOrMore>
          <ref name="m.li"/>
        </oneOrMore>
      </element>
    </define>
    <define name="m.ul">
      <element name="ul">
        <oneOrMore>
          <ref name="m.li"/>
        </oneOrMore>
      </element>
    </define>
    <define name="m.li">
      <element name="li">
        <text/>
      </element>
    </define>
    <define name="m.blockquote">
      <element name="blockquote">
        <text/>
      </element>
    </define>
  </div>
  <div>
    <define name="m.htmlCharMarkup">
      <choice>
        <text/>
        <ref name="m.a"/>
        <ref name="m.img"/>
        <ref name="m.br"/>
        <ref name="m.strong"/>
        <ref name="m.b"/>
        <ref name="m.em"/>
        <ref name="m.i"/>
      </choice>
    </define>
    <define name="m.a.href">
      <attribute name="href">
        <data type="anyURI"/>
      </attribute>
    </define>
    <define name="m.a.attlist">
      <ref name="m.a.href"/>
    </define>
    <define name="m.a">
      <element name="a">
        <ref name="m.a.attlist"/>
        <ref name="m.htmlCharMarkup"/>
      </element>
    </define>
    <define name="m.img.src">
      <attribute name="src">
        <data type="anyURI"/>
      </attribute>
    </define>
    <define name="m.img.alt">
      <attribute name="alt"/>
    </define>
    <define name="m.img.attlist">
      <ref name="m.img.src"/>
      <optional>
        <ref name="m.img.alt"/>
      </optional>
    </define>
    <define name="m.img">
      <element name="img">
        <ref name="m.img.attlist"/>
        <text/>
      </element>
    </define>
    <define name="m.br">
      <element name="br">
        <empty/>
      </element>
    </define>
    <define name="m.strong">
      <element name="strong">
        <text/>
      </element>
    </define>
    <define name="m.b">
      <element name="b">
        <text/>
      </element>
    </define>
    <define name="m.em">
      <element name="em">
        <text/>
      </element>
    </define>
    <define name="m.i">
      <element name="i">
        <text/>
      </element>
    </define>
  </div>
